#!/bin/bash

SERVICE_STATE=$1
SERVICE_STATE_TYPE=$2
SERVICE_ATTEMPT=$3
USER_PATH=$4
echo "$SERVICE_STATE"
echo "$SERVICE_STATE_TYPE"
echo "$SERVICE_ATTEMPT"
echo "$USER_PATH"

BBB_HOST=$5
BBB_PORT=$6
BBB_SALT=$7
echo "$BBB_HOST:$BBB_PORT"

/usr/bin/python $USER_PATH/bigbluebutton/bigbluebutton.py $BBB_HOST:$BBB_PORT $BBB_SALT &> ~/output
